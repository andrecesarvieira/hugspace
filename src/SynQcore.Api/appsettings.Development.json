{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=synqcore_db;Username=synqcore_user;Password=synqcore_dev_password;Include Error Detail=true",
    "Redis": "localhost:6379"
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": false,
    "StackBlockedRequests": false,
    "HttpStatusCode": 429,
    "IpWhitelist": [ "127.0.0.1", "::1", "192.168.0.0/16", "10.0.0.0/8" ],
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1m", 
        "Limit": 1000
      }
    ]
  },
  "ClientRateLimiting": {
    "EnableEndpointRateLimiting": false,
    "StackBlockedRequests": false,
    "HttpStatusCode": 429,
    "ClientIdHeader": "X-ClientId",
    "ClientWhitelist": [ "*" ],
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1m",
        "Limit": 5000
      }
    ]
  }
}
